{
    "private": true,
    "name": "creativeworkspace",
    "description": "My private website",
    "author": "Markus Sommer",
    "version": "1.0.1",
    "scripts": {
        "build": "cross-env NODE_ENV=production nuxt build",
        "dev": "nuxt",
        "generate": "yarn generate:blog && yarn generate:github && cross-env NODE_ENV=production nuxt generate",
        "lint": "eslint --ext .js,.ts,.vue .",
        "start": "nuxt start",
        "generate:blog": "./bin/getBlogData",
        "generate:github": "./bin/getGithubData"
    },
    "dependencies": {
        "@nuxtjs/axios": "5.9.5",
        "@nuxtjs/pwa": "3.0.0-beta.20",
        "@vue/composition-api": "0.4.0",
        "countup.js": "2.0.4",
        "eslint-plugin-vue": "6.2.1",
        "isomorphic-unfetch": "3.0.0",
        "lazy-hydration": "1.0.4",
        "nuxt": "2.11.0",
        "sanitize.css": "11.0.0",
        "vue-countup-v2": "4.0.0",
        "vue-eslint-parser": "7.0.0",
        "vue-lazy-hydration": "1.0.0-beta.12",
        "vue-lazyload": "1.3.3",
        "vue-observe-visibility": "0.4.6"
    },
    "devDependencies": {
        "@nuxtjs/dotenv": "1.4.1",
        "@nuxtjs/eslint-config": "2.0.2",
        "@nuxtjs/eslint-module": "1.1.0",
        "@nuxtjs/google-tag-manager": "2.3.1",
        "@nuxtjs/markdownit": "1.2.7",
        "@nuxtjs/netlify-files": "1.0.0",
        "@nuxtjs/sitemap": "2.0.1",
        "babel-eslint": "10.0.3",
        "contentful": "7.13.1",
        "cross-env": "7.0.0",
        "cssnano": "4.1.10",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-nuxt": "0.5.1",
        "eslint-plugin-prettier": "3.1.2",
        "glob": "7.1.6",
        "highlight.js": "9.18.1",
        "husky": "4.2.3",
        "markdown-it": "10.0.0",
        "markdown-it-link-target": "1.0.2",
        "nuxt-jsonld": "1.4.5",
        "nuxt-payload-extractor": "0.0.14",
        "nuxt-seo": "1.2.2",
        "nuxt-webfontloader": "1.1.0",
        "postcss-css-variables": "0.14.0",
        "postcss-responsive-type": "1.0.0",
        "prettier": "1.19.1",
        "stylelint": "13.2.0",
        "stylelint-config-recommended": "3.0.0",
        "stylelint-config-standard": "20.0.0"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    }
}
